{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Data Archive Model Catalog",
  "description": "Catalog of all schemas in the Data Archive Model",
  "type": "object",
  "properties": {
    "schemas": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "version": {
            "type": "string"
          },
          "url": {
            "type": "string",
            "format": "uri"
          }
        },
        "required": ["name", "version", "url"]
      }
    }
  },
  "required": ["schemas"],
  "schemas": [
    {
      "name": "Producer",
      "url": "./Producer.json"
    },
    {
      "name": "Deposit",
      "url": "./Deposit.json"
    },
    {
      "name": "SIP",
      "url": "./SIP.json"
    },
    {
      "name": "IntellectualEntity",
      "url": "./IntellectualEntity.json"
    },
    {
      "name": "Representation",
      "url": "./Representation.json"
    },
    {
      "name": "File",
      "url": "./File.json"
    },
    {
      "name": "Fixity",
      "url": "./Fixity.json"
    }
  ]
}